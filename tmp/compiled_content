  <h1>SteedOS Admin API</h1>
  <div id="fast">Coming soon</div>
</section>
      </div>
      </div>
    </div>
    <div id="tabs-back"></div>
  </article>  

    </div>
    <div id="modals">
      
    </div>
  </div>
