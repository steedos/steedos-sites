#modals {
  position: absolute; /* Ensure that modals don't screw up sticky footer */
  z-index: 100;
}

#modals section.modal-pane {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  top: 0;
}

#modals section.modal-pane .modal-background {
  position: fixed;
  background-color: rgba(0, 0, 0, 0.7);
  bottom: 0;
  left: 0;
  right: 0;
  top: 0;
}

#modals section.modal-pane .modal-content {
  background-color: #fbfbfb;  
	border: 1px solid #2a2a2a;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  border-radius: 4px;
  -webkit-box-shadow: 0 0 10px rgba(0,0,0,.33);
  -moz-box-shadow: 0 0 10px rgba(0,0,0,.33);
  box-shadow: 0 0 10px rgba(0,0,0,.33);
  color: #3B3B3B;
  left: 50%;
  padding: 18px 50px 10px;
  position: fixed;
  top: 50%;
}

